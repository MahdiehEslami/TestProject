@model List<AppRole>

    <table class="table table-bordered table-striped">
        <tr>
            <td>Title</td>
        </tr>
        @if (Model?.Any() == true)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                </tr>
            }
        }
        else
        {
            <tr><td>Role Not Found</td></tr>
        }
    </table>